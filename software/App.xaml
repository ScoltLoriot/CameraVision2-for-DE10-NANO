<Application x:Class="CameraVision2.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CameraVision2"
             StartupUri="MainWindow.xaml" DispatcherUnhandledException="Application_DispatcherUnhandledException">
    <Application.Resources>
        <ResourceDictionary>
            <SolidColorBrush x:Key="colorDark" Color="#FF0A0A0A" />
            <SolidColorBrush x:Key="colorLight" Color="#FFF0F0F0" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
